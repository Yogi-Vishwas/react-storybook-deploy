{
  "name": "react-boilerplate-ts-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@axa-fr/react-oidc": "^6.0.12",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.94",
    "@mui/material": "^5.10.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "formik": "^2.2.9",
    "notistack": "^2.0.5",
    "oidc-client": "^1.11.5",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-idle-timer": "^5.4.1",
    "react-query": "^3.39.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "recharts": "^2.1.13",
    "tinycolor2": "^1.4.2",
    "use-react-router-breadcrumbs": "^3.2.1",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "config": "node ./src/Configuration/configureEnv.js",
    "start": "npm run config && react-scripts start",
    "build": "npm run config && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src/",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "lint:fix": "eslint --fix  --ext .js,.jsx,.ts,.tsx ./src/",
    "test:all": "cross-env set CI=true && react-scripts test && npm run lint",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "coverage": "cross-env CI=true npm test -- --env=jsdom --coverage",
    "cypress": "cypress open"
  },
  "lint-staged": {
    "*.+(js|jsx)": "eslint --fix",
    "*.+(json|css|md)": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/index.js",
      "src/reportWebVitals.js",
      "src/App.js"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@mui/types": "^7.1.5",
    "@testing-library/cypress": "^8.0.3",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.4",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/tinycolor2": "^1.4.3",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cypress": "^10.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "typescript": "^4.7.4"
  }
}
